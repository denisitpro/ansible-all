disable_cache = true
disable_mlock = true
ui = true
listener "tcp" {
   address          = "{{vault_listener_addr}}"
   tls_disable      = 1
}
storage "file" {
   path  = "{{vault_data_path}}"
 }
api_addr         = "{{vault_api_addr}}"
max_lease_ttl         = "10h"
default_lease_ttl    = "10h"
cluster_name         = "vault"
raw_storage_endpoint     = true
disable_sealwrap     = true
disable_printable_check = true