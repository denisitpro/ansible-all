         via: 10.125.0.1
       - to: 10.25.0.0/16
         via: 10.125.0.1
       - to: 10.30.0.0/16
         via: 10.125.0.1
       - to: 10.31.0.0/16
         via: 10.125.0.1
       - to: 10.32.0.0/16
         via: 10.125.0.1
       - to: 172.31.0.0/16
         via: 10.125.0.1
